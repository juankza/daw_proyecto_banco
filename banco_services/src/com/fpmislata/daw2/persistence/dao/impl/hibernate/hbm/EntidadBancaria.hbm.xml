<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class name="com.fpmislata.daw2.business.domain.EntidadBancaria" table="entidadbancaria">
        
        <id column="idEntidadBancaria" name="idEntidadBancaria">
             <generator class="identity" />
        </id>
        
        <property column="nombre" name="nombre" />
        
        <property column="codigoEntidadBancaria" name="codigoEntidadBancaria" />
        
        <property column="fechaCreacion" name="fechaCreacion" />
        
        <property column="direccion" name="direccion" />
        
        <property column="cif" name="cif" />
        
    </class>
    
</hibernate-mapping>
