<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.fpmislata.daw2.business.domain.MovimientoBancario" table="movimientobancario">
        <id column="idMovimientoBancario" name="idMovimientoBancario" />
        <property column="tipo" name="tipo">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.fpmislata.daw2.business.domain.TipoMovimientoBancario</param>
                <param name="type">12</param>
            </type>
        </property>
        <property column="concepto" name="concepto" />
        <property column="cantidad" name="cantidad" />
        <property column="fecha" name="fecha" />
        <many-to-one name="idCuentaBancaria">
            <column name="idCuentaBancaria"  />
        </many-to-one>
 
    </class>
</hibernate-mapping>
